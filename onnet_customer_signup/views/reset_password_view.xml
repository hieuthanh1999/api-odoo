<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="inherit_views_signin_reset" name="Onnet Subscription Account List"
              inherit_id="auth_signup.reset_password">
             <xpath expr="//p[hasclass('alert-danger')]" position="replace">
                <p class="alert alert-danger" t-if="error" role="alert">
                    <span>To request for a new reset password email, please write to us at <a href="/contactus" target="_blank">Contact Us</a>. Thank you.</span>
                </p>
            </xpath>
    </template>
</odoo>