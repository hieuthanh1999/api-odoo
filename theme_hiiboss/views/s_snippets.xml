<template id="service_slider_layout" name="Service Slider Snippet Layout">
    <section class="dynamic-services-snippet home-services-snippet">
        <div class="container">
            <div class="swiper as-Swiper">
                <div class="swiper-wrapper">
                    <t t-foreach="services" t-as="service">
                        <div class="swiper-slide">
                            <div class="feature-box" t-attf-style="background-color: #{service.service_id.bg_color};">
                                <h5><t t-esc="service.service_id.name"/></h5>
                                <div class="feature-icon">
                                    <img t-att-alt="service.service_id.name" t-att-src="website.image_url(service.service_id,'icon')" t-att-title="service.service_id.name"/>
                                </div>
                                <div class="feature-content">
                                    <div class="text">
                                        <p><t t-esc="service.service_id.description"/></p>
                                    </div>
                                    <div class="btn-bar">
                                        <a class="arrow-link-white" t-att-href="service.service_id.link" target="_blank">View more</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>
                <div class="hi_swiper-button-next"></div>
                <div class="hi_swiper-button-prev"></div>
            </div>
        </div>
    </section>
</template>
